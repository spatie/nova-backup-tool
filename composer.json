{
    "name": "spatie/nova-backup-tool",
    "description": "A Laravel Nova tool to backup your application.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "homepage": "https://github.com/spatie/nova-backup-tool",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.48.23|^11.35",
        "laravel/nova": "^5.0",
        "nesbot/carbon": "^2.62.1|^3.4",
        "spatie/laravel-backup": "^8.0|^9.0"
    },
    "require-dev": {
        "laravel/nova-devtool": "^1.4",
        "orchestra/testbench": "^8.30|^9.7",
        "spatie/phpunit-snapshot-assertions": "^5.1.7"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Spatie\\BackupTool\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\BackupTool\\Tests\\": "tests",
            "Workbench\\App\\": "workbench/app/",
            "Workbench\\Database\\Factories\\": "workbench/database/factories/",
            "Workbench\\Database\\Seeders\\": "workbench/database/seeders/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\BackupTool\\BackupToolServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "post-autoload-dump": [
            "@clear",
            "@prepare"
        ],
        "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": "@php vendor/bin/testbench workbench:build --ansi",
        "serve": [
            "Composer\\Config::disableProcessTimeout",
            "@build",
            "@php vendor/bin/testbench serve --ansi"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}